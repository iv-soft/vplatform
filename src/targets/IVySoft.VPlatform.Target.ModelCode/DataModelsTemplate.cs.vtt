@inherits IVySoft.VPlatform.Target.ModelCode.DataModelContext
@using Microsoft.EntityFrameworkCore
@{
	foreach(var module in Model.Modules)
	{
		if(!module.IsExternal)
		{
			var folder = System.IO.Path.Combine(Options.OutputFolder, module.Namespace);
			System.IO.Directory.CreateDirectory(folder);

			foreach(var entity_type in module.Types)
			{
				Generate_EntityType(module, entity_type, System.IO.Path.Combine(folder, entity_type.Name + ".cs"));
			}
		}
	}
}